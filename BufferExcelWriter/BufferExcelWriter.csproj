<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.1.0</Version>
    <Authors>itoktsnhc@hotmail.com</Authors>
    <Description>BufferExcelWriter</Description>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Itoktsnhc/BufferExcelWriter</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Itoktsnhc/BufferExcelWriter</RepositoryUrl>
    <PackageReleaseNotes>impove flexibility, now you can upload sheet relationship multi time, and dynamic update sheet headers</PackageReleaseNotes>
    <PackageVersion>0.1.0</PackageVersion>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Copyright>itoktsnhc@hotmail.com</Copyright>
    <Company />
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>Excel</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="exceltemplate" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="exceltemplate" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.1" />
  </ItemGroup>

</Project>
